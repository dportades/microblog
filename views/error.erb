<div class="error_wrapper">
  <h2><%= flash[:alert] %></h2>
  <h2><%= flash[:notice] %></h2>
  <br>
  <br>
  <h3>If you have an account, please sign in again.</h3>
  <form method="get" action="/">
    <input type="submit" value="Sign In" class="err_btn">
  </form>
  <br>
  <br>
  <h3>Click here to sign up!</h3>
  <form method="get" action="/index">
    <input type="submit" value="Sign Up" class="err_btn">
  </form>
  <br>
</div>


 